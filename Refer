import os

def find_sql_files(start_dir='.'):
    for root, dirs, files in os.walk(start_dir):
        for file in files:
            if file.endswith('.sql'):
                relative_path = os.path.relpath(os.path.join(root, file), start_dir)
                print(relative_path)

# Specify the starting directory here if it's different from the current directory
start_directory = '.'
find_sql_files(start_directory)
